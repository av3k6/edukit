&lt;p&gt;&lt;strong&gt;&lt;span data-contrast="auto" xml:lang="EN-US" lang="EN-US" class="TextRun SCXW193285922 BCX0"&gt;&lt;span class="NormalTextRun SCXW193285922 BCX0"&gt;Order an EduKit Today!&lt;/span&gt;&lt;/span&gt;&lt;span class="EOP SCXW193285922 BCX0" data-ccp-props="{&amp;quot;201341983&amp;quot;:0,&amp;quot;335559739&amp;quot;:160,&amp;quot;335559740&amp;quot;:259}"&gt;&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;
